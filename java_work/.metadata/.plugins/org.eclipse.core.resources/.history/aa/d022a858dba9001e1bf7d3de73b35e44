package ex03;

public class Ex08 {

	// 1byte -128 ~ +127
	
	public static void main(String[] args) {
		char aa = 'A';
		System.out.println(aa);
		int bb = (int) aa;
		System.out.println(bb);
		char cc = (char) bb;
		System.out.println(cc);
		
		byte kk = 127;
		System.out.println(kk+(byte)1);
	}
}
